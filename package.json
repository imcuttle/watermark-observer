{
  "name": "watermark-observer",
  "version": "1.0.0",
  "description": "Watermark effect with dom observer",
  "author": "余聪 <yucong06@meituan.com>",
  "scripts": {
    "test": "npx jest",
    "test:watch": "npm test -- --watch",
    "preversion": "npm test",
    "build": "npm run clean && run-p --print-label \"build:**\"",
    "example": "cd example && npx webpack serve",
    "dev": "TSC_OPTIONS=\"--watch\" npm run build",
    "build:es": "tsc $TSC_OPTIONS --outDir es --module es6",
    "build:cjs": "tsc $TSC_OPTIONS --outDir lib",
    "build:tds": "tsc $TSC_OPTIONS --emitDeclarationOnly -d",
    "clean": "rimraf types es lib",
    "prepare": "npm run build",
    "version": "npm run changelog",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "sideEffects": false,
  "engines": {
    "node": ">=10"
  },
  "files": [
    "es",
    "types",
    "lib",
    "src"
  ],
  "keywords": [
    "余聪",
    "watermark",
    "observer",
    "detect",
    "watermark-observer"
  ],
  "main": "lib",
  "types": "types",
  "license": "MIT",
  "repository": "余聪/watermark-observer",
  "module": "es",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testMatch": [
      "**/__test{s,}__/*.(spec|test).{t,j}s{x,}"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jest": "^26.0.15",
    "conventional-changelog-cli": "^2.1.1",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "style-to-js": "^1.1.0"
  }
}
